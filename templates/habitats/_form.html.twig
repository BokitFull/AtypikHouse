{{ form_start(form, { 'attr': {'id': 'form'}}) }}
        <div class="col-12 form-errors">
            {{ form_errors(form) }}
        </div>

        <div id="form-input-div" class="row col-12">
            {{ form_row(form._token) }}
            <div class="form-group col-md-6">
                {{ form_row(form.titre, { 'attr': {'class': 'input-field', 'placeholder': 'Titre'} }) }}
            </div>
            <div class="form-group col-md-6">
            {{ form_row(form.adresse, { 'attr': {'class': 'input-field', 'placeholder': 'Adresse'} }) }}
            <input type="hidden" name="adresse">
            <div id="result_adresse"></div>
            </div>
            <div class="form-group col-md-6">
            {{ form_row(form.code_postal, { 'attr': {'class': 'input-field ', 'placeholder': 'Code postal'} }) }}
            </div>
            <div class="form-group col-md-6">
            {{ form_row(form.nb_personnes, { 'attr': {'class': 'input-field', 'placeholder': 'Nombre personnes max'} }) }}
            </div>
            <div class="form-group col-md-6">
            {{ form_row(form.debut_disponibilite, { 'attr': {'class': 'input-field', 'placeholder': 'Nombre personnes max'} }) }}
            </div>
