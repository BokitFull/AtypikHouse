<form method="post" action="{{ path('app_habitats_delete', {'id': habitat.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer cet habitat ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ habitat.id) }}">
    <button class="btn" title="Delete"><i class="fa-solid fa-circle-xmark delete"></i></button>
</form>
