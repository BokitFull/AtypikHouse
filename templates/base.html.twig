<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}

    </head>
    <header>
        <div class="container-80">
            <nav class="nav-col">
                <div class="nav-col-logo">
                    <a href="/">
                        <img src="{{ asset('images/atypik_long-removebg-preview.654ec52f.png') }}" class="nav-logo">
                    </a>
                </div>
                <div id="nav-col-menu">
                    <input id="menu-toggle" type="checkbox" />
                    <label id="menu-btn" for="menu-toggle">
                        <span id="first-pan"></span>
                        <span id="burger"></span>
                        <span id="second-pan"></span>
                    </label>
                    <ul id="menu-box">
                        <li class="nav-item">
                            <a class="nav-link fs-5 green-text" href="/">Destinations <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fs-5 green-text" href="/">Hébergements <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fs-5 green-text" href="/">Contact<span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item d-md-none">
                            <a class="nav-link fs-5 green-text">Devenir hôte<span class="sr-only">(current)</span></a>
                        </li>
                    </ul>
                </div>
            </nav>
        <div>
    </header>
    <body>
        <div>
            {% block content %}{% endblock %}
        </div>
    </body>
</html>
